EST_LDA_COM ::= RACI IGU MORE:e2 MEN  Cuerpo_LDA:e1  ES_LDA2 {:e1.accion=e2.toString(); RESULT=e1;:}
			//  Accion = {[^]} >                <   /   Lista Datos  >   
			|	MEN  Cuerpo_LDA:e1  ES_LDA2					{:RESULT=e1;:}
			| 	MEN  error 			ES_LDA2					{:ListaDatos nuevo = new ListaDatos(); RESULT=nuevo;:}
			|	error MEN  Cuerpo_LDA:e1  ES_LDA2			{:RESULT=e1;:}
			;	
			//  >                <   /   Lista Datos  >  
			
ES_LDA2 ::= MAY  DIA RLDA   		MEN
		|	MAY  DIA error 	RLDA   	MEN
		|	MAY  DIA RLDA 	error  	MEN
		|	MAY  DIA error 			MEN
		|	MAY  error 				MEN
			;
			
Cuerpo_LDA ::= 	Cuerpo_LDA:e2  ES_DA:e1		{:e2.Datos.add(e1); RESULT=e2;:} 
			|	ES_DA:e1					{:ListaDatos nuevo = new ListaDatos(); nuevo.Datos.add(e1); RESULT=nuevo;:}
			;

/*---------------------------- Etiqueta Dato----------------------------YA CON RECUPERACION DE ERRORES*/	
ES_DA ::= 	MAY RDAT  EST_DA_COM:e1 {:RESULT=e1;:};
		//	<   Dato  estructura   
EST_DA_COM ::= REFE IGU CAD:e2    	MEN SUM_TEXT:e1  	ES_DA2 	{:EDato nuevo= new EDato(); nuevo.Dato=e1; nuevo.linea=e2left+1; nuevo.columna=e2right+1; nuevo.referencia=e2.toString(); RESULT=nuevo;:}
			// REFERENCIA = "[^]" >             <   /      Dato  >
			|						MEN SUM_TEXT:e1 	ES_DA2	{:EDato nuevo= new EDato(); nuevo.Dato=e1; nuevo.linea=e1left+1; nuevo.columna=e1right+1; RESULT=nuevo;:}

			| error ES_DA2	{:EDato nuevo= new EDato(); RESULT=nuevo;:};

