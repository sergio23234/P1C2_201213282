			|MEN SUM_TEXT:e1 MAY  DIA RDAT   MEN									{:EDato nuevo= new EDato(); nuevo.Dato=e1; nuevo.linea=e1left+1; nuevo.columna=e1right+1; RESULT=nuevo;:}
				;
			
