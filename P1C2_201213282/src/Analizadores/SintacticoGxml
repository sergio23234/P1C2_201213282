			|	ES_DA:e1					{:ListaDatos nuevo = new ListaDatos(); nuevo.Datos.add(e1); RESULT=nuevo;:}
			;

/*---------------------------- Etiqueta Dato----------------------------*/	
ES_DA ::= 	MAY RDAT  EST_DA_COM:e1 {:RESULT=e1;:};
